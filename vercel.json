{
  "version": 2,
  "buildCommand": "npm install --no-optional && NODE_OPTIONS='--max-old-space-size=4096' next build",
  "installCommand": "npm install --no-optional",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production"
  }
}
